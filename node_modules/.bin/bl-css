#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/DELL/dump/fg_xd/bl-css-gh/node_modules/.pnpm/bl-css@0.10.1/node_modules/bl-css/bin/node_modules:/proc/cygdrive/c/Users/DELL/dump/fg_xd/bl-css-gh/node_modules/.pnpm/bl-css@0.10.1/node_modules/bl-css/node_modules:/proc/cygdrive/c/Users/DELL/dump/fg_xd/bl-css-gh/node_modules/.pnpm/bl-css@0.10.1/node_modules:/proc/cygdrive/c/Users/DELL/dump/fg_xd/bl-css-gh/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/DELL/dump/fg_xd/bl-css-gh/node_modules/.pnpm/bl-css@0.10.1/node_modules/bl-css/bin/node_modules:/proc/cygdrive/c/Users/DELL/dump/fg_xd/bl-css-gh/node_modules/.pnpm/bl-css@0.10.1/node_modules/bl-css/node_modules:/proc/cygdrive/c/Users/DELL/dump/fg_xd/bl-css-gh/node_modules/.pnpm/bl-css@0.10.1/node_modules:/proc/cygdrive/c/Users/DELL/dump/fg_xd/bl-css-gh/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/bl-css@0.10.1/node_modules/bl-css/bin/sc.js" "$@"
else
  exec node  "$basedir/../.pnpm/bl-css@0.10.1/node_modules/bl-css/bin/sc.js" "$@"
fi
